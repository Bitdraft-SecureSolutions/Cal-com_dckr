#POSTGRES_PASSWORD=Spli4809hfeSVKq5ly31JAWE8s8jBFCH
#POSTGRES_DB=agenda_consultorio_cal_com_db
#DATABASE_HOST=dpg-d1m8js6mcj7s73a00r6g-a
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${DATABASE_HOST}/${POSTGRES_DB}
#DATABASE_URL=postgresql://agenda_consultorio_cal_com_db_user:Spli4809hfeSVKq5ly31JAWE8s8jBFCH@dpg-d1m8js6mcj7s73a00r6g-a/agenda_consultorio_cal_com_db
